{
  "expo": {
    "name": "IPTV Pro",
    "slug": "iptv-pro",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#1a1a1a"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.seudominio.iptvpro"
    },
    "android": {
      "package": "com.seudominio.iptvpro",
      "permissions": [
        "android.permission.INTERNET",
        "android.permission.ACCESS_NETWORK_STATE",
        "android.permission.WAKE_LOCK"
      ],
      "allowBackup": false
    },
    "web": {
      "bundler": "metro"
    },
    "plugins": [
      "expo-video"
    ]
  }
}
